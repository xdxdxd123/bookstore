<script type="text/javascript" charset="utf-8" src="http://static.bshare.cn/b/buttonLite.js#style=-1&uuid=&pophcol=2&lang=zh"></script>
<script type="text/javascript" charset="utf-8" src="http://static.bshare.cn/b/bshareC0.js"></script>
    <script>
        var path = "../";
    </script>
</head>

<body>
<div id="topBar" class="topBar">
  <div class="screen topBar-ctn gray clearfix">
     <!--分享按钮 登录前显示-->
     <div id="bShare" class="f_left">
       <div class="bshare-custom bshare-icon"><a title="分享到QQ空间" class="bshare-qzone"></a><a title="分享到新浪微博" class="bshare-sinaminiblog"></a><a title="分享到人人网" class="bshare-renren"></a><a title="分享到腾讯微博" class="bshare-qqmb"></a><a title="分享到网易微博" class="bshare-neteasemb"></a><a title="更多平台" class="bshare-more bshare-more-icon more-style-addthis"></a></div>
     </div>
    <!--欢迎登录 登录后显示-->
     <div class="welcome f_left none">
       <span class="block f_left">您好&nbsp;&nbsp;欢迎来到Bookstore!</span>
       <div class="f_left pr">
          <a class="userName" href="javascript:void(0);"></a>
          <a class="loginOut ml5" href="javascript:void(0);">退出</a>
       </div>
     </div>
     <div class="topBar-menu f_right">
       <!--作者机构空间登录后显示-->
       <div class="welcomeRoom f_left none">
         <span class="block f_left">您好&nbsp;&nbsp;</span>
         <div class="f_left pr">
           <a class="userName" href="javascript:void(0);"></a>
           <a class="userAccount gray" href="index.html">我的账户</a>
           <a class="userIndent gray" href="index.html">我的订单</a>
           <a class="loginOut ml5" href="javascript:void(0);">退出</a>
           <!--<a href="http://www.gzebook.cn/zone/authorReg.html" class="author_apply gray none">作者申请</a>
           <a href="http://www.gzebook.cn/zone/pressReg.html" class="press_apply gray none">出版社机构申请</a>
           <a href="http://www.gzebook.cn/zone/comedy.html?usertype=1" class="author_house gray none">作者空间</a>
           <a href="http://www.gzebook.cn/zone/comedy.html?usertype=2" class="press_house gray none">出版社空间</a>
           <a class="gray" href="http://www.gzebook.cn/index.html">广购书城</a>-->
         </div>
       </div>
       <!--登录前显示-->
       <span class="gotoLogin f_left">您好&nbsp;&nbsp;欢迎来到Book!<a href="login.html" class="bule" style="margin-left:10px;">请登录</a><a href="http://www.gzebook.cn/user/register.html" class="gray" style="margin-left:10px;">免费注册</a></span>
       <a class="topBar-cart gray f_left" href="shoppingCart.html">购物车<em id="cartNum" class="bule fb" style="margin-left:6px;">0</em></a>
       <!--登录后显示-->
       <p class="topBar-menu-logined f_left none">
         <!--<a id="author_apply" href="http://www.gzebook.cn/zone/authorReg.html" class="gray none">作者申请</a>
         <a id="press_apply" href="http://www.gzebook.cn/zone/pressReg.html" class="gray none">出版社机构申请</a>
         <a id="author_house" href="http://www.gzebook.cn/zone/comedy.html?usertype=1" class="gray none">作者空间</a>
         <a id="press_house" href="http://www.gzebook.cn/zone/comedy.html?usertype=2" class="gray none">出版社空间</a>-->
         <a href="#" class="gray">联系客服</a>
       </p>
       <!--登录前显示-->
        <a href="#" class=" f_left pl20 gray">网站导航</a>
     </div>
  </div>
</div>
<div id="wrap">
  <!-- logo图标欢迎注册 -->
  <div id="header">
    <div class="screen pr">
      <div class="clearfix">
        <h1 class="f_left mt10 pr20"><a href="index.html"><img src="images/logo.png" /></a></h1>
        <span class="name-login f20 pl20 mt16 f24">欢迎注册</span>
      </div>
    </div>
  </div>
  <!-- 内容 -->
  <div id="content" class="screen mt24">
      <div class="register pr f14">
          <div class="pa toggle">
              <span class="toggle-info toggle-left conterttoggle">手机注册</span>
              <span class="toggle-info toggle-right">邮箱注册</span>
          </div>
          <span class="f_right">我已经注册，现在就<a href="login.html" style="color:#3333FF;">登录</a></span>
      </div>
      <form id="mobileForm" isError='false' action="" class="register-form">
          <div class="form-list1">
              <ul class="register-ul f14">
                  <li>
                      <label for=""><em class="color-red mr10">*</em>用户名：</label><input name="username" id="mUsername" type="text" class="{required:true}" />
                      <p class="checkTips none f_left ml20 color-red">! 用户名已经被使用</p>
                  </li>
                  <li>
                      <label for=""><em class="color-red mr10">*</em>密码：</label><input name="userpwd" id="mUserpwd" type="password" class="{required:true,PassWord:true}" autocomplete="off" />
                  </li>
                  <li>
                      <label for=""><em class="color-red mr10">*</em>确认密码：</label><input name="surepwd" id="mSurepwd" type="password" class="{required:true,surepwd:'#mUserpwd'}" autocomplete="off" />
                  </li>
                  <li>
                      <label for="">电子邮箱：</label><input name="email" id="mEmail" type="text"/>
                  </li>
                  <li>
                      <label for=""><em class="color-red mr10">*</em>手机号码：</label><input name="mobile" id="mMobile" type="tel" class="{required:true,tel:true}"/>
                      <p class="checkTips none f_left ml20 color-red">! 手机号已经被使用</p>
                  </li>
                  <li>
                      <label for=""><em class="color-red mr10">*</em>验证码：</label><input name="code" id="mCode" type="text" class="captcha {required:true}" />
                      <span id="getMobileCode" class="obtain f13">获取短信验证码</span>
                  </li>
              </ul>
              <div class="terms-service" style="margin-bottom:38px">
                  <p class="f_left" style="width:190px; height:14px; display:block;"></p>
                  <span class="f_left">
                       <input type="checkbox" class="checkbox-info f14" id="mMale" /><label for="mMale" class="ml5 color_333">同意</label>
                       <a href="#" class="provision ml5">《数字书城服务条款》</a>
                  </span>
              </div>
              <div class="terms-service">
                  <p class="f_left" style="width:190px; height:14px; display:block;"></p>
                  <button type="submit" class="button buttonClass f16 color_fff bdrs_2" id="mButton">立即注册</button>
              </div>
          </div>
      </form>
      <form id="emailForm" validate='true' isError='false' action="" class="register-form none">
          <div class="form-list2">
              <ul class="register-ul f14">
                  <li>
                      <label for=""><em class="color-red mr10">*</em>用户名：</label><input name="username" id="eUsername" type="text" class="{required:true}" />
                      <p class="checkTips none f_left ml20 color-red">! 用户名已经被使用</p>
                  </li>
                  <li>
                      <label for=""><em class="color-red mr10">*</em>密码：</label><input name="userpwd" id="eUserpwd" type="password" class="{required:true,PassWord:true}" autocomplete="off"/>
                  </li>
                  <li>
                      <label for=""><em class="color-red mr10">*</em>确认密码：</label><input name="surepwd" id="eSurepwd" type="password" class="{required:true,surepwd:'#eUserpwd'}" autocomplete="off"/>
                  </li>
                  <li>
                      <label for="">手机号码：</label><input name="mobile" id="mMobile" type="tel" class="{tel:true}"/>
                      <p class="checkTips none f_left ml20 color-red">! 手机号已经被使用</p>
                  </li>
                  <li>
                      <label for=""><em class="color-red mr10">*</em>电子邮箱：</label><input name="email" id="eEmail" type="text" class="{required:true,email:true}" />
                      <p class="checkTips none f_left ml20 color-red">! 电子邮箱已经被使用</p>
                  </li>
              </ul>
              <div class="terms-service" style="margin-bottom:38px">
                  <p class="f_left" style="width:190px; height:14px; display:block;"></p>
                  <span class="f_left">
                       <input type="checkbox" class="checkbox-info f14" id="eMale" /><label for="eMale" class="ml5 color_333">同意</label>
                       <a href="#" class="provision ml5">《数字书城服务条款》</a>
                  </span>
              </div>
              <div class="terms-service">
                  <p class="f_left" style="width:190px; height:14px; display:block;"></p>
                  <button type="submit" class="button buttonClass f16 color_fff bdrs_2" id="eButton">立即注册</button>
              </div>
          </div>
      </form>
  </div>
</div>
<div id="phone-pic-vild" class="pic-vild-wrap none">
  <div class="pic-vild-overlay"></div>
  <div class="pic-vild-content">
    <p class="pic-vild-title">
      请输入验证码
      <span class="pic-vild-close">×</span>
    </p>
    <div class="pic-vild-from">
        <div class="clearfix">
            <input class="f_left pic-vild-ipt" />
            <img class="f_left pic-vild-img" />
        </div>
        <p class="pic-vild-tip none">验证码错误，请重新输入</p>
        <div class="pic-vild-btn">确定</div>
    </div>
  </div>
</div>
</body>
<script type="text/javascript" src="js/jquery-1.12.0.min.js"></script>
<script type="text/javascript" src="js/core/mmd.js"></script>
<script type="text/javascript" src="js/core/util.js"></script>
<script type="text/javascript" src="js/core/common.js"></script>
<script type="text/javascript" src="js/plugins/validation/jquery.validate.min.js"></script>
<script type="text/javascript" src="js/plugins/validation/jquery.metadata.js"></script>
<script type="text/javascript" src="js/plugins/validation/message_zhCN.js"></script>
<script type="text/javascript" src="js/plugins/validation/additional-methods.js"></script>
<script type="text/javascript" src="js/register.js"></script>
</html>